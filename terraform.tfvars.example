environment                        = "dev"
location                           = "usgovvirginia"
azure_environment                  = "usgovernment"
synapse_sql_administrator_login    = "synapseadmin"
synapse_sql_administrator_password = "ReplaceWithStrongPassword123!"

existing_user_assigned_identity = {
  name                = "existing-identity-name"
  resource_group_name = "existing-identity-rg"
}

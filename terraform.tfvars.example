  name                = "existing-keyvault-name"
  resource_group_name = "existing-keyvault-rg"
}
existing_key_vault_key = {
  name = "existing-key-name"
}
enable_private_dns_zones = false

tags = {
  owner      = "data-team"
  costcenter = "1234"
}
